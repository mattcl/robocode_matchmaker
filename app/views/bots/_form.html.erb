<%= simple_form_for(@bot) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :jar_file, :as => :file %>
    <%= f.association :categories, :as => :check_boxes, :required => true %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, 'Upload' %>
  </div>
<% end %>
